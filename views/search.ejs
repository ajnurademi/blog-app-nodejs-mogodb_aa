<div class="author">
    <h1 class="author__heading">Search Results</h1>
  </div>
  
  <!--showing the blogs from the searchTerm-->
  <section class="articles">
  
    
    <ul class="article-ul">
      <% data.forEach(post => { %>
        <li>
          <a href="/post/<%= post._id %>">
            <span><%= post.title %></span>
            <span class="article-list__date"><%= post.createdAt.toDateString() %></span>
          </a>
        </li>
      <% }) %>
    </ul>
  
  
  </section>